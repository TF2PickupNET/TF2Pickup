[declarations]
.*/node_modules/.*
private/.*

[ignore]

[include]

[libs]

[lints]
all=error

[options]
emoji=true
experimental.const_params=true
module.use_strict=true
include_warnings=true
module.file_ext=.js
module.file_ext=.json
module.file_ext=.jpg
module.file_ext=.ico
module.file_ext=.mp3
module.name_mapper.extension='mp3' -> '<PROJECT_ROOT>/stubs/mp3.js'
module.name_mapper.extension='ico' -> '<PROJECT_ROOT>/stubs/favicon.js'

[strict]

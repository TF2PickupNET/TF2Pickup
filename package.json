{
  "private": true,
  "scripts": {
    "lint": "eslint . --ext js,jsx",
    "flow": "check-flow --color=always",
    "test": "NODE_ENV=\"TEST\" ava",
    "test:coverage": "nyc yarn run test",
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "clean": "lerna clean",
    "lerna": "lerna"
  },
  "browserslist": [
    "Firefox >= 59",
    "Chrome >= 65"
  ],
  "dependencies": {
    "@babel/cli": "7.0.0-beta.51",
    "@babel/core": "7.0.0-beta.51",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.51",
    "@babel/plugin-proposal-export-default-from": "7.0.0-beta.51",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0-beta.51",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.51",
    "@babel/preset-env": "7.0.0-beta.51",
    "@babel/preset-flow": "7.0.0-beta.51",
    "@babel/preset-react": "7.0.0-beta.51",
    "@babel/register": "7.0.0-beta.51",
    "ava": "1.0.0-beta.5.1",
    "babel-loader": "8.0.0-beta.3",
    "browser-env": "3.2.5",
    "chalk": "2.4.1",
    "check-flow": "1.0.1",
    "connect-history-api-fallback": "1.5.0",
    "css-loader": "0.28.11",
    "date-fns": "1.29.0",
    "eslint": "5.0.0",
    "eslint-config-henribeck": "5.0.4",
    "file-loader": "1.1.11",
    "flow-bin": "0.74.0",
    "flow-copy-source": "2.0.0",
    "fs-extra": "6.0.1",
    "html-webpack-plugin": "3.2.0",
    "koa-connect": "2.0.1",
    "lerna": "2.11.0",
    "mini-css-extract-plugin": "0.4.0",
    "npmlog": "4.1.2",
    "nyc": "12.0.2",
    "promise-serial": "0.1.5",
    "raf": "3.4.0",
    "rimraf": "2.6.2",
    "shelljs": "0.8.2",
    "sinon": "6.0.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.0.1",
    "webpack": "4.12.0",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-merge": "4.1.3"
  },
  "nyc": {
    "check-coverage": true,
    "extension": [
      ".jsx"
    ],
    "exclude": [
      "packages/**/*.spec.js",
      "tests/**/*"
    ],
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "report-dir": "./.coverage"
  },
  "devDependencies": {
    "babel-plugin-import": "1.8.0"
  }
}
